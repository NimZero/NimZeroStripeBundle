{
    "name": "nimzero/stripe-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony Bundle to integrate Stripe.",
    "keywords": ["Symfony", "bundle", "Stripe"],
    "homepage": "https://github.com/NimZero/NimZeroStripeBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nimzero",
            "email": "contact@nimzero.fr"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/http-kernel": "^5.4 | ^6.0",
        "symfony/config": "^5.4 | ^6.0",
        "symfony/dependency-injection": "^5.4 | ^6.0",
        "symfony/framework-bundle": "^5.4 | ^6.0",
        "symfony/event-dispatcher": "^5.4 | ^6.0",
        "symfony/console": "^5.4 | ^6.0",
        "symfony/polyfill-php80": "^1.26",
        "doctrine/orm": "^2.13",
        "doctrine/doctrine-bundle": "^2.7",
        "twig/twig": "^3.4",
        "stripe/stripe-php": "^9.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "phpstan/phpstan-symfony": "^1.2",
        "symfony/maker-bundle": "^1.0",
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^5.4 | ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Nimzero\\StripeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nimzero\\StripeBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": [
            "vendor/bin/phpstan",
            "vendor/bin/phpunit"
        ]
    }
}
