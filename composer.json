{
    "name": "nimzero/stripe-bundle",
    "type": "symfony-bundle",
    "description": "A Stripe helper for Symfony application",
    "license": "MIT",
    "authors": [
        {
            "name": "NimZero",
            "email": "contact@nimzero.fr"
        }
    ],
    "require": {
        "php": ">=8.0",
        "stripe/stripe-php": ">=10.0.0",
        "symfony/http-kernel": "^5.4 | ^6.0",
        "symfony/dependency-injection": "^5.4 | ^6.0",
        "symfony/config": "^5.4 | ^6.0",
        "symfony/console": "^5.4 | ^6.0",
        "doctrine/orm": "^2.14",
        "symfony/security-bundle": "^5.4 | ^6.0",
        "twig/twig": "^3.5"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.48"
    },
    "autoload": {
        "psr-4": {
            "Nimzero\\StripeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nimzero\\StripeBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
